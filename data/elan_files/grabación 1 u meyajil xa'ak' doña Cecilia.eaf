        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a2" TIME_SLOT_REF1="ts5" TIME_SLOT_REF2="ts6">
                <ANNOTATION_VALUE>ku, kin preparaartke xa'ak'o' esten... kin ts'áaik aajo, pimyeenta, sebooya, esten... káanela, pimyeenta Tabaasko yéete komiino, óoreganoj, tu láakle je'elo' kin ts'áak ichij.</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a3" TIME_SLOT_REF1="ts7" TIME_SLOT_REF2="ts8">
                <ANNOTATION_VALUE>Ti'al u... máak'antik, tin ts'áak ich ki'iwaj ti'alu ts'áak u sáabor ti'e ki'iwajo', </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a4" TIME_SLOT_REF1="ts9" TIME_SLOT_REF2="ts10">
                <ANNOTATION_VALUE>je'e bix ku beeta'ale e ki'iwajo', ku beeta'a eskabeeche, esten... chirmoole, reyenoj, esten... bisteek yéete, yéete tak óonsikil.</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a6" TIME_SLOT_REF1="ts11" TIME_SLOT_REF2="ts12">
                <ANNOTATION_VALUE>ya'abu klaasesi esten... ki'iwaj ku beeta'a yéete, e kin, chenin preparaarte' ti' u p'ée, un p'ée ka' kin juch'ik, úuchben , úuchben klaase e, e ka' tu'uxkin juch'ko'.</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a7" TIME_SLOT_REF1="ts15" TIME_SLOT_REF2="ts16">
                <ANNOTATION_VALUE>Elo' aanimasin in, in sweegra p'atmij, leti'o' p'at ten  ti'ain in, in máak'antik xa'ak', tin juch'ik xa'ak' ka tu p'ato' ten.</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a8" TIME_SLOT_REF1="ts17" TIME_SLOT_REF2="ts18">
                <ANNOTATION_VALUE>utsi úuchben kensa tu'ux tu cha'o',</ANNOTATION_VALUE>

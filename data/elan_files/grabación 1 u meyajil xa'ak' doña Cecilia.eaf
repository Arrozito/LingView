            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a9" TIME_SLOT_REF1="ts19" TIME_SLOT_REF2="ts20">
                <ANNOTATION_VALUE>elo' ma' chen ma', ma' je'e bixe ku, ku ko'ono' ti', ti' áartesaniyaobo' elo' jach, jach de tuunich máak'anta'anik.</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a10" TIME_SLOT_REF1="ts23" TIME_SLOT_REF2="ts24">
                <ANNOTATION_VALUE>Pus bey ya'abo' ku ts'áak taku p'ée semaana pero wa ma'e' yaan oorae' chenu, chenu p'íit kin máak'anki' chen ti'alun p'íit ki'iwaj chen, yaan oorae' chenu p'ée aajo kin máak'anku xa'ak'ij, láaj ts'áaku u tu láakle ba'axo' ku bisko' chen ti'alu u p'ée chan ooya ki'iwaj kin máak'antik beyo', pus ma' ya'ab kin máak'anki'</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a11" TIME_SLOT_REF1="ts25" TIME_SLOT_REF2="ts26">
                <ANNOTATION_VALUE>wa kin máak'antik ya'abe' pus elo' ku, ku xáanta ten, ku beetik taku p'ée semaana.</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a12" TIME_SLOT_REF1="ts29" TIME_SLOT_REF2="ts30">
                <ANNOTATION_VALUE>¿Bixu foormai?, e aajo yan... yanin mukik, chenin muke' kin ts'íilik, ts'oke' kin, kin láaj p'ayláantik take sebooyao' kin láaj ts'áaki',</ANNOTATION_VALUE>

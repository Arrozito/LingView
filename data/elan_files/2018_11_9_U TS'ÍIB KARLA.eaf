    <TIER ANNOTATOR="[APELLIDO, NOMBRE]" LINGUISTIC_TYPE_REF="TRANSCRIPCIÓN" PARTICIPANT="[APELLIDO, NOMBRE]" TIER_ID="H1_Transcripción">
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a1" TIME_SLOT_REF1="ts1" TIME_SLOT_REF2="ts2">
                <ANNOTATION_VALUE>Tene' in k'aaba'e' Karla Yaneli Uc Nahuat, tene' tin xook tu najil xook Unibersidaad de Oryeente ti' kwaarto kwatrimeestre.</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a2" TIME_SLOT_REF1="ts3" TIME_SLOT_REF2="ts4">
                <ANNOTATION_VALUE>Tene' tin mi... tin meyaj yo'osal u p'ée teema u k'aaba'e' k'eex, pos a woojlile'e'xe' e k'eexo' esten... ti' espanyoole' u k’áat u ya’ale’ “el cambio de alma”.</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a3" TIME_SLOT_REF1="ts5" TIME_SLOT_REF2="ts6">
                <ANNOTATION_VALUE>Tene' bix uchku súurjir esten... bix uchku yanchaj ten íinteres yo'osale teema, pus deke ten tin chichnile' ee... meyáajta'a’ ten k'eex, tso'okole' yan xan ts'in iku meeta'al tin naayi' mene le yaan oorae' in soobo'obe' yaan oorae' ku chujko' iik' k'aas take’,pus ku k’oja’antalo’ob, ku yantal chokwi ti’ob, ba’ax… ba’alob beyo’.</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a4" TIME_SLOT_REF1="ts7" TIME_SLOT_REF2="ts8">
                <ANNOTATION_VALUE>pwes a woojle'exe' deke e ba'a... e ba'a chen úuchko' esten... chen yéete k'eex je'elu páajtal u utstale'.</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a5" TIME_SLOT_REF1="ts9" TIME_SLOT_REF2="ts10">
                <ANNOTATION_VALUE>Ee... ka'achile' pus ma' in wooje ba'ax jachu u... u séerbire ba'a beya' asta ka ooken xooke' pos tin k'ajóotaj deke ya'akach ritwaales ku meeta'a.</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a6" TIME_SLOT_REF1="ts11" TIME_SLOT_REF2="ts12">
                <ANNOTATION_VALUE>Ee... e tune k'eexa' bix ts'in a'ak te'exa' pwes uts tin wich, taak in kanik maas. Tak beore' in woojlile' ya'akach ee... ya'akach kaawsas yo'osal ku meeta'ale e k'eexo'.</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a7" TIME_SLOT_REF1="ts13" TIME_SLOT_REF2="ts14">
                <ANNOTATION_VALUE>Yaan oorae' ku meyáajta'a deke bey chenu chuk este iik' k'aasu túu máake', yaan oorae' ku… ku ya'ala'ale' pus leete' chen jo'oko' de tak de áak'abo', tak las doose, wa yaan oora ku ya'ala'ale' chen máanko' ti'u esten... tséelu u p'ée che', u p'ée ch'e'en,</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a8" TIME_SLOT_REF1="ts15" TIME_SLOT_REF2="ts16">
                <ANNOTATION_VALUE>yaan oora xane' wa ku máano'ob tu'uxan máak kala'ano’ob, ba'alo' beyo'.</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a9" TIME_SLOT_REF1="ts17" TIME_SLOT_REF2="ts18">
                <ANNOTATION_VALUE>Ee... pwes chen chu'ukuk tun mene iik' k'aaso' ee... e máako' pwes ku káajlu k'i'inamu pool yaan oora,</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a10" TIME_SLOT_REF1="ts19" TIME_SLOT_REF2="ts20">
                <ANNOTATION_VALUE>yaan oora xane' ku ts'áaik chokwi ti', yaan oorae' tak táankabij.</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a11" TIME_SLOT_REF1="ts21" TIME_SLOT_REF2="ts22">
                <ANNOTATION_VALUE>Wa mun meyáajta'alu kweentaile ba'a bey ku yúuchlo' je'elu páajtal taku u meetku kimle winko' menelo' u p'ée ba'a k'aas oka'anti' </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a12" TIME_SLOT_REF1="ts23" TIME_SLOT_REF2="ts24">
                <ANNOTATION_VALUE>e ba'a tuna' esten... yaan oora ku ya'ala'ale' chen ikna doktore' mun… mun utsta, mun utskúunkech chen yéete ts'aak asta paak'i'a bin ikna juntu... juntúul jmeene' ku... yáaxile' ku jo'oska sweerte chen ts'o'ojke' wa u yilmaj wa chen chuka'anech men iik'e' pos ku ya'ake' yanu meyáajtik teche k'eexo'.</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a13" TIME_SLOT_REF1="ts25" TIME_SLOT_REF2="ts26">
                <ANNOTATION_VALUE>E k'eexo' ku meyáajta'a yéete jump'éel... yaan oorae' yéete jkaax, yaan oorae' yéete páaloma ba'alo' beyo', yaan xan ooare' ma' chen yéete aalak'i' sinoke yaan oorae' yéete je' wa yéete estenu u... ja'as.</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a14" TIME_SLOT_REF1="ts27" TIME_SLOT_REF2="ts28">
                <ANNOTATION_VALUE>Esten... pwese' e chen usáarta'ak e kaaxo'obo' u p'ée remplaaso ti'e' máak k'oja'ano' menele' chen meyáajta'ak u k'eexilo' tu láakle iik' k'aas yan, e yan tu ichile máako' pwes ku máan te' aalak'o' wa te' je'o' wa ba'axa'ake'.</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a15" TIME_SLOT_REF1="ts29" TIME_SLOT_REF2="ts30">
                <ANNOTATION_VALUE>Esten...</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a16" TIME_SLOT_REF1="ts31" TIME_SLOT_REF2="ts32">
                <ANNOTATION_VALUE>Tu ts'ooki tune' pwes chenu jaantu wa… wa u túu kaaxe' chenu jaante e máak ti'… ti’ me… meyáajta'abe' k'eexo' tu ts'ookile' tu láaklu baake, tu láaklu u k'u'uk' le kaaxo' ku líisa'a, ku to'oko' wa ku ts'áaba ti' u p'ée chan kaaja,</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a17" TIME_SLOT_REF1="ts33" TIME_SLOT_REF2="ts34">
                <ANNOTATION_VALUE>chen ts'o'okok tune' ku bisa'a mukbi ti' jáal solar,</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a18" TIME_SLOT_REF1="ts35" TIME_SLOT_REF2="ts36">
                <ANNOTATION_VALUE>mene bey bino' u representasyooni de ke sáan kin... sáan kimik, beorae' u p'ée túumben máak, beorae' mina'an esten... ba'a k'aas ichilu wíinkli sáan utsak beyo'.</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a19" TIME_SLOT_REF1="ts37" TIME_SLOT_REF2="ts38">
                <ANNOTATION_VALUE>Yaan xan oorae' ku meeta'a ti' tu'ux... tu'ux yanu p'ée kool</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a20" TIME_SLOT_REF1="ts39" TIME_SLOT_REF2="ts40">
                <ANNOTATION_VALUE>Menele’ yaan oorae' e... e máako' káampesinoso'obo' kolnáalo'obo' pwees ku… ku sajbéesalo'ob mene mejen aluxo'obo' wa yaan oorae' chen iik' k'aastak xan beetik,</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a21" TIME_SLOT_REF1="ts41" TIME_SLOT_REF2="ts42">
                <ANNOTATION_VALUE>yaan oorae' pwes e bixe' te' chena chuk esten... iik' k'aaso' ku ts'a'aya xan ti' chokwi, k'i'inan pool. </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a22" TIME_SLOT_REF1="ts43" TIME_SLOT_REF2="ts44">
                <ANNOTATION_VALUE>Yaan ooare' chen ku sajbéesa'alo', e bey xano' wa mun... wa man meetka u, u... kweentaile' pwes beyku séegiro',</ANNOTATION_VALUE>

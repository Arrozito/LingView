        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a1" TIME_SLOT_REF1="ts1" TIME_SLOT_REF2="ts2">
                <ANNOTATION_VALUE>Tene' in k'aaba'e' Felipe te' in taa tu kaajil muchukuxkaje' tu munisipyoi chank'oom.Tene' in k'aaba'e' Felipe te' in taa tu kaajil muchukuxkaje' tu munisipyoi chank'oom.</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a2" TIME_SLOT_REF1="ts3" TIME_SLOT_REF2="ts4">
                <ANNOTATION_VALUE>.Bike'en in tsikbat te'ex o'osalu experyeensyai yanchajten ka tin meeti tenin serbiisyo komunitaaryo tu kaajil kokail ti' konaafej.</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a3" TIME_SLOT_REF1="ts5" TIME_SLOT_REF2="ts6">
                <ANNOTATION_VALUE>Ka yáax k'uchene' tu… te' kaajo', ba'ax tin yáax beete' pus yáax binenin wile estene e presideentaile eskweelao',</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a4" TIME_SLOT_REF1="ts7" TIME_SLOT_REF2="ts8">
                <ANNOTATION_VALUE>chen ba'ale' pwes ku yanta techu u saajaki men mixu puul meyajnakech yéete mejen paala mix yéete paadres de fáamilya.</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a5" TIME_SLOT_REF1="ts9" TIME_SLOT_REF2="ts10">
                <ANNOTATION_VALUE>Komoke chen todabiya chen paalen tene' ku yanta techu p'ée… u p'ée nojoch tuukul ka wa'ale' pwes ba'ax kun úuchu ma'.</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a6" TIME_SLOT_REF1="ts11" TIME_SLOT_REF2="ts12">
                <ANNOTATION_VALUE>.Ka tun an yax binene', binen in wilej ka tu ya'aj tene' maestro wa ts'o'ok a k'uchle' ka ik meyajto'on jump'éel réeunyoon, ka laj júuntarnaj tu láakle papaso'obo' ka tu ya'alo' tene' wa teen ts'o'ok in k'uchu bine' esten te' miismo diyao' pus yanu déesignaarta'a ten tu'ux ken p'áataj, ka tya'alo' tun ten máax naay ken bin.</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a7" TIME_SLOT_REF1="ts13" TIME_SLOT_REF2="ts14">
                <ANNOTATION_VALUE>Ka ts'o'oke' ka sáaschaje' ka yáax kajin ts'áak tune xooko',</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a8" TIME_SLOT_REF1="ts15" TIME_SLOT_REF2="ts16">
                <ANNOTATION_VALUE>ka in laj k'ajóolte mejen paalalo'obo', oocho mejen paala tokaarnaj ten.</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a9" TIME_SLOT_REF1="ts17" TIME_SLOT_REF2="ts18">
                <ANNOTATION_VALUE>Pus u jaajile' em... p'ée níibel yaniko' uts men láaju yoojlo'o’ ts'íib', máas o meenos abansaado le ku kaniko'ob xook.</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a10" TIME_SLOT_REF1="ts19" TIME_SLOT_REF2="ts20">
                <ANNOTATION_VALUE>U k'aaba' e mejen paalalo'obo' pwes... Victor, Karla, Josmar, an... Joselin, Alexander, Victoria, Yair yéete Yanet, letu k'aaba' tune mejen paala tokaarnaj teno'.</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a11" TIME_SLOT_REF1="ts21" TIME_SLOT_REF2="ts22">
                <ANNOTATION_VALUE>Te yáax… te' yáax k'iinak beyo' pus leti’obe’ sajko'o’ ti' teen men ma' u k'ajóoleno'obi',</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a12" TIME_SLOT_REF1="ts23" TIME_SLOT_REF2="ts24">
                <ANNOTATION_VALUE>em...  bey tun uchu máane k'iino'obo', ka ts'o'oku enu jump'éel meese' tu sigyeente meesile' pwes ka wike' an... ku taa taku méek'eche mejen paalalo'obo' jump'ée ba'a pus ma’ suukan techi' pwes ekstraanyo.</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a13" TIME_SLOT_REF1="ts25" TIME_SLOT_REF2="ts26">
                <ANNOTATION_VALUE>Chen taako'obe' pus ku méek'kecho'ob ku ya'ako' teche' maestro, yaan oorae' tyoorai táan ts'áake' xooko' ku tu'ubu tyo'obe' ku ya'ako' teche' papaj ma maama beyu ya'ako' techo',</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a14" TIME_SLOT_REF1="ts27" TIME_SLOT_REF2="ts28">
                <ANNOTATION_VALUE>am... ka k'uch tunu k'iini ti' am... nobyeembre bweno ti' septyeembre nobyeembre pus ku meeta' desfiile, pus tene' kin meetik desfiiles yéetelo' ya'abkach ba'a tin meeto'on yéetelo' .</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a15" TIME_SLOT_REF1="ts29" TIME_SLOT_REF2="ts30">
                <ANNOTATION_VALUE>U p'ée aktibidaad máan uts tin wilaj tene' lete' tiale jaanal pixano' tin meyajta yéetelo'obo' men tene' tin kaajale' jela'anin wik… jela'anin wiku meeta'a, ka tin xookin preepa tu kaajil chank'oome' yaana ten bix… bix uchin wiku beeta'a.</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a16" TIME_SLOT_REF1="ts31" TIME_SLOT_REF2="ts32">
                <ANNOTATION_VALUE>Te' tun te chan kaaj jela'an xanu meetko' leti'o'. Leti'obe' ku laj béestirta'ale mejen paalalob de saku nook'o'obo', ma to wa'alaj ba'ax k'iin kena meete' chenu k'axo'obe chan… chenin il ts'o'oku k'a'axle  naj mene estene... paadres de fáamilyao' beya' ka laj taasa'abe mejen paalalo'obo', laj béestidoo’ túun pero ba'alile' u chan desben, bweno u béentajai' te'elo' meene mejen paalalo'obo' utsile' ku laj máanu k'aato'ob ba'a bey te' te’ tu'uxun naaye mejen nukuch máako’ob, ma meeta' píib, ma duulsei k'úum, iis tu láaka beyo' , ku máanu k'aato'ob beyo' leti' túun kun ts'áabi te' áaltaro'.</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a17" TIME_SLOT_REF1="ts33" TIME_SLOT_REF2="ts34">
                <ANNOTATION_VALUE>.Ba'axu láa xan  uts tin wile' pus chen meeta'ak e aktibidaado' e paadres de fáamillyao' leti'obe' mun importaaro' ma na'anu mejen paala te'elo' ma yane', tu láakle kaaj ku taalo' ku júuntarko'obe' ku meetkobu nobeenai tu láaka ts'o'oke' ku chen t'o'oxo tu láakle ba'ax meeta'abo' ti'a xan tu láaka jachu p'éel… jachup'ée aktibiidad jats'uts ten tin wilaj.</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a18" TIME_SLOT_REF1="ts35" TIME_SLOT_REF2="ts36">
                <ANNOTATION_VALUE>U la' p'ée ba'ale' jach jump'éel kaaj ku tráatarkech um... beyu paarteech ti' leti'obe' beyu l'áak'echo'obe' beyu yiku iijoecho'obe' men u la'a p'ée aktibiidade' lete' ka, chen káajkene... primeero de disyeembre asta doose de disyeembreo' te' kóokailo' sáansamalu kíinsa'a k'éeke'ni'.</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a19" TIME_SLOT_REF1="ts37" TIME_SLOT_REF2="ts38">
                <ANNOTATION_VALUE> Chen kíinsake k'éek'en sáansamalo' pwes am... tak kex ma' ta  ts'áak xook tu mejen paalalo'ob beya' ku t'ankecho' jaana, chenu ya'alo' teche' “maestro ko'ox jaana” pus yana bini', doose dyaas tun yanu tookara jaantik am... le bu'uli bak', chooch, táaman, reyeeno, kochiniita, lete' bey ku ts'áaba tech sáansamalo', p'áat jach uts ten tin wili lelo'. </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a20" TIME_SLOT_REF1="ts39" TIME_SLOT_REF2="ts40">
                <ANNOTATION_VALUE>Pus beytun uchu séegire aanyobo' asta ka, ka xan a'alatene' am... pus yanin p'atke kaajo' tumen te' conafeo' ts'o'oku ya'ala tene'  yani… yani yani...yani, a'ala ten wa je'elin beetkinba kapasitadoorile' pero chen ba'ale' u p'éelu desbentaajaile' a'ala tene' yanin p'atke mejen paalalo' aanteso',</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a21" TIME_SLOT_REF1="ts41" TIME_SLOT_REF2="ts42">
                <ANNOTATION_VALUE> myéentras tune' tu láakle ba'alob kin meetko'on yéetelo'obo' pus kox a'ake' chen ko'on, chen bey ko'on pule' ma', men mas ma' tun séerbir to'on, ka' túun tin wale' ma'táan mix tin aseptaartik , </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a22" TIME_SLOT_REF1="ts43" TIME_SLOT_REF2="ts44">
                <ANNOTATION_VALUE>ka je'elen tun te' kóomunidad beyo' pus ka séegirnajeni'.</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a23" TIME_SLOT_REF1="ts45" TIME_SLOT_REF2="ts46">
                <ANNOTATION_VALUE>Te', te' yáax primer aanyo pwes ya'a ba'a tin kani', tin kanaj meyaj yéetele paadres de fáamilyao', tin seguundo aanyo tune' pwes maas uts uchku bin ten men ts'o'oku yanta ten kóonfyansa yéetelo', ts'o'oku yanta ten um... ko'ox a'ake ba'a'la'... ya'ab ba'a ts'o'okin k’ajóotik te' bixu ka'ansa' mejen paala, ka'achile' u p'ée ba'a ma'in woojli'.</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a24" TIME_SLOT_REF1="ts47" TIME_SLOT_REF2="ts48">
                <ANNOTATION_VALUE>Tin seguundo aanyoe' pwes yanchaj tenu óoprtunidadilin bino'on tak yéetele mejen paalalo' te' soona áarkeolojika chichen tak te'e tin biso'ono’obi',</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a25" TIME_SLOT_REF1="ts49" TIME_SLOT_REF2="ts50">
                <ANNOTATION_VALUE>tumen yo'osalu máas k'aóotko’obu entoorno ko'ox a'a'ke' le xooko' ma' déebe seeru ts'áaba chen ich sáaloni' k'a'abet u jo'osale mejen paalalo'ob yo'salu eksperimentaarko' tu láakle ba'a yan ajan ko'ox a'ake' tu alrededooro'obo'.</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a26" TIME_SLOT_REF1="ts51" TIME_SLOT_REF2="ts52">
                <ANNOTATION_VALUE>Bey tun uchak xan ik bisko'onobo' pus tin júuntartikbáaon yéetelin kompanyeerso'obo' yetu tu máaestro xan ku ts'áa xook tu kaajil xa'anla'  yéetelu tu xch'úupa ku ts'aa xook tu kaajil xk'a'atun, </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a27" TIME_SLOT_REF1="ts53" TIME_SLOT_REF2="ts54">
                <ANNOTATION_VALUE>ka tin múuchkíinsibáaone' ka bino'on t'aan yéetele presideenteo' ka tu ya'alaj to'one' pus je'elu yáanko'one' tu túuxtaj to'onu p'ée kamyoneeta, ki ik meeto'on u liistaile' mejen paala ko'on biso'obo' ka xan tik a'alo'on ka tun tik laj biso'ono'. </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a28" TIME_SLOT_REF1="ts55" TIME_SLOT_REF2="ts56">
                <ANNOTATION_VALUE>Ka k'ucho'on te' komunidaad xan tu'ux kin ts'áa xooko' ka tin wa'alaj ti'o' pus yanu preparaarkubáaob ku méet esten u ko'ox a'ake' u yo'oche mejen paalalo'obo' ku biso' pus men te'elo' ko'oj ba'ali'.</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a29" TIME_SLOT_REF1="ts57" TIME_SLOT_REF2="ts58">
                <ANNOTATION_VALUE>Ka sáaschaj tune' pus ka ooko'on ch'a'abi to'on mene kamyono', ka máano'on xan tu kaaji xk'a'atun in ch'a'in kompayeera yéetel áalumnoso' pus ka bino'on te kaajo', </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a30" TIME_SLOT_REF1="ts59" TIME_SLOT_REF2="ts60">
                <ANNOTATION_VALUE>te' k'iin tuno' pus jach jump'ée jach uts tin wilaj men a… óoxp'ee eskweela bey conbibiirnajo’.</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>

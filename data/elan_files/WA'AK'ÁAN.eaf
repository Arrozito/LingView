    <TIER ANNOTATOR="[APELLIDO, NOMBRE]" LINGUISTIC_TYPE_REF="TRANSCRIPCIÓN" PARTICIPANT="[APELLIDO, NOMBRE]" TIER_ID="H1_Transcripción">
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a1" TIME_SLOT_REF1="ts1" TIME_SLOT_REF2="ts2">
                <ANNOTATION_VALUE>Tene' in k'aaba'ae' Marcela, in éedade' de trentainweebe anyos,</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a2" TIME_SLOT_REF1="ts3" TIME_SLOT_REF2="ts4">
                <ANNOTATION_VALUE>in dedikáarminba' ti' wa' k'áan.</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a3" TIME_SLOT_REF1="ts5" TIME_SLOT_REF2="ts6">
                <ANNOTATION_VALUE>¿Tu'ux kin manku iiloi? saki', ¿ba'ax kóoloresi?, segun ba'ax kóoloresi ku k'áata'a tene' leti' kin in máak'antik.</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a4" TIME_SLOT_REF1="ts7" TIME_SLOT_REF2="ts8">
                <ANNOTATION_VALUE>Chen ts'o'ojke' kin abisaarke e persoona k'áatmilo' ku taalu ch'a'e'.</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a5" TIME_SLOT_REF1="ts9" TIME_SLOT_REF2="ts10">
                <ANNOTATION_VALUE>I bix… esten... ku taalu cha'e' wa segun máaxti' utschaj tu yiche' pwes ku komunikaarkuba ti' máax u jeelu k'ajóolale' ku taalu yil waye' kin máak'antik in woojen ba'ax... ya'abu klaasesi dibuujos u kóoloresi yéete bixu u máak'anan a ka'axik yaan ba'aj.</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a6" TIME_SLOT_REF1="ts11" TIME_SLOT_REF2="ts12">
                <ANNOTATION_VALUE>Yana táan óotik mu' k'astaj, men wa ku k'astale' ku… ku kóochtalu yáamo'. </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a7" TIME_SLOT_REF1="ts13" TIME_SLOT_REF2="ts14">
                <ANNOTATION_VALUE>I pus e persoonas ku taa iko' yéete k’a'wa'mayormeente way ku taalo'obe', yaan ku taa p'iste'i', Káalak'op.</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a8" TIME_SLOT_REF1="ts15" TIME_SLOT_REF2="ts16">
                <ANNOTATION_VALUE>Je'e ba'ax kóolorilu u… u k'áato'obe' pwes leti' kin in máak'antik i ¿ba’ax esten tamaanyoi? de oocho, de dyees wa de doose.</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a9" TIME_SLOT_REF1="ts17" TIME_SLOT_REF2="ts18">
                <ANNOTATION_VALUE>Chen leti' kin in dedikáarkinbi in máak'antej,</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a10" TIME_SLOT_REF1="ts19" TIME_SLOT_REF2="ts20">
                <ANNOTATION_VALUE>chen tso'okok ba'alin beet, in líimpyarkin naay, ba'ax beya' leti' kin in meetik men chen leti' in chan, chen leti' in chan meyaj te' way ichnáaje'. </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a11" TIME_SLOT_REF1="ts21" TIME_SLOT_REF2="ts22">
                <ANNOTATION_VALUE>Le beetke' pus ya'a máak ts'u'u k'a´'óotkin meyaje' pus ku taalu yileno' yaan ts'u'u taa esten... tu ka'alak'o'opi ikene' k'áatik wa je'elin máak'anke' pus tin wa'aj ti'e' je'ele'.</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a12" TIME_SLOT_REF1="ts23" TIME_SLOT_REF2="ts24">
                <ANNOTATION_VALUE>chen tunin in ts'o'okse e le de doose je'ela' pos leti' kenin máak'ante ts'oke' yanu túu… yanu tu esten... k'awa'i' ts'u'u taa ikene' u k'áatu p'ée de dyeesi' leti' tun keni’in… in máak'antej, kex jujump'íiti' pero tu binubin.</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a13" TIME_SLOT_REF1="ts25" TIME_SLOT_REF2="ts26">
                <ANNOTATION_VALUE>U kóolori tuno', u kóoloresi ma' in… in mani' pero yanin binin manej, ¿tu'ux kin manik? saki',</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a14" TIME_SLOT_REF1="ts27" TIME_SLOT_REF2="ts28">
                <ANNOTATION_VALUE>buka'aju máateryali kenin manej ti'u kweerpoi, ti' u kaal yéete esten... ti'ilin in esten... estenin in… in esten... in káajsik. </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a15" TIME_SLOT_REF1="ts29" TIME_SLOT_REF2="ts30">
                <ANNOTATION_VALUE>¿I bixu káajba? yanin yáax k'axik te' paache che'o' u ts'oke' yanin kanki k'axu sa’a’chi' men wa me' chen… chen to'ok'ajku u ti'ilu káajbalo' ku k'asta tene' pus yanin wik bex kenin in...</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a16" TIME_SLOT_REF1="ts31" TIME_SLOT_REF2="ts32">
                <ANNOTATION_VALUE>Ka káajene' pos xaan tin wilin kanik pero asta  ka tin in kanin káajsu u… u ti'lu k'áaxa tak ti'lu  ts'o'okoj u estenu u kweerpoi, tu láaka beyo'.</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a17" TIME_SLOT_REF1="ts33" TIME_SLOT_REF2="ts34">
                <ANNOTATION_VALUE>Pus tene' chen leti' in, in chan meyaj, pus chen náajkin wóolin in wak', in wak'in k'aane'  pus kin chan chitaj, chen náajkin wóo chitle' pus kin ka líik'ij, chen leti' ki’in dedikáarkinbaj pus chen bey… bey jach  k'a'ana'ano' pus yanin wikin in ts'o'oksik sée men wa ma'a'e' ken taak ilbi wa ma' ts'o'ojke' pwes ka'amáal ken taa ilbij.</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a18" TIME_SLOT_REF1="ts35" TIME_SLOT_REF2="ts36">
                <ANNOTATION_VALUE>Este ni’in... la'aj… la’a chenin láaj k'ubláantej, p'aata ten óoxp'eli’in u binin máak'antik ka béeyakin in láaj k'ubke e… e k'a'anin k'u’láanko'.</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a19" TIME_SLOT_REF1="ts37" TIME_SLOT_REF2="ts38">
                <ANNOTATION_VALUE>Láaj difereentes dibujos y láaj difereentes kóoloresu u k'áatko' ten, </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a20" TIME_SLOT_REF1="ts39" TIME_SLOT_REF2="ts40">
                <ANNOTATION_VALUE>ti'e' je'elo' pus tu ya'alo'obe' yanu komunikaarkubáaob ti' u jeel máaxu k'aóolo'obe' men ya'ab bin persoonas taaku manku k'aano'be' pwes na'an tu'ux, yaan tu'ux ku ko'ono bine’ pero ma' je'e bix ku déebe seere', </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a21" TIME_SLOT_REF1="ts41" TIME_SLOT_REF2="ts42">
                <ANNOTATION_VALUE>Le beetik tune' tu ya'alo' tene' wa kin meyajtike' ku taalu yileno'obe' kia'ako ten ba'ax… ba'ax kóolorilu k'áato'obe' pwes kin manik te' esten... saki'o' te' kin manki',</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a22" TIME_SLOT_REF1="ts43" TIME_SLOT_REF2="ts44">
                <ANNOTATION_VALUE>Le beetke' pus chen leti' kin chan dedikáarkinbaj ti' tu láaka, chen leti' in chan meyaj.</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
    </TIER>

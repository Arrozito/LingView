    <TIER ANNOTATOR="[APELLIDO, NOMBRE]" LINGUISTIC_TYPE_REF="TRANSCRIPCIÓN" PARTICIPANT="[APELLIDO, NOMBRE]" TIER_ID="H1_Transcripción">
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a1" TIME_SLOT_REF1="ts1" TIME_SLOT_REF2="ts2">
                <ANNOTATION_VALUE>Tene' esten... in k'aaba'e'... esten Luis Alberto Moo Yam, yaan ten trentiseeys anyos.</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a2" TIME_SLOT_REF1="ts3" TIME_SLOT_REF2="ts4">
                <ANNOTATION_VALUE>Pus tene' estén… kin kíinsik k'éek'en</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a3" TIME_SLOT_REF1="ts5" TIME_SLOT_REF2="ts6">
                <ANNOTATION_VALUE>pues leete'e tu ka'ansaj tene nochoj máako' pus in papaje' leti' a'ale' kin kane' men ma' tin kane' xooko'</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a4" TIME_SLOT_REF1="ts7" TIME_SLOT_REF2="ts8">
                <ANNOTATION_VALUE>ka tu ya'aj tene' pus xeen tu paache nojoch máako' y kane, ka  kan le kíinsak k'e'eno' </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a5" TIME_SLOT_REF1="ts9" TIME_SLOT_REF2="ts10">
                <ANNOTATION_VALUE>pus tene' leti' tin kanaj esten te' duraante tyeempo, seeys años meyajnajen yéete otsi nojoch máako' ka tin utsi kantaj.</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a6" TIME_SLOT_REF1="ts11" TIME_SLOT_REF2="ts12">
                <ANNOTATION_VALUE>bo'ortase' tin juuna kin kaxkin kuxtaj esten...</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a7" TIME_SLOT_REF1="ts13" TIME_SLOT_REF2="ts14">
                <ANNOTATION_VALUE>pus bo'ortase' le e chenin chinin in in kiinse e k'éek'en bo'ortae' pus yáax kin kiinsik te'e yalan tu'ux yan u xiik' beya' kin kiinsik, despweese' chen kinke' kin chokoja'atik yéetele e je'elo' kin júuplin jo'ochej,</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a8" TIME_SLOT_REF1="ts15" TIME_SLOT_REF2="ts16">
                <ANNOTATION_VALUE>chen ts'o'kok tunin jo'ochik tun beyo' kin jo'osku bak'ij, kin bujik u chúumuku nak' beya' kin jo'osku choochij, despweese' kin kin tsíilku k'ewlilo', kin jo'osku bak'ij,</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a9" TIME_SLOT_REF1="ts17" TIME_SLOT_REF2="ts18">
                <ANNOTATION_VALUE>chen ts'o'okok tun te'e je'elo', despweese' kin... kin tsajik, yáaxu tsaatsi kin ts'aaik beyo', despweese' kin ts'áaku mejen xiixbak'elo', chen ts'o'ojkin ts'áak tunu u... u sóol beyo' kin júuplin júuytik despaasyo yo'osa mu' táak'a ten kin ts'áaik k'áak' te' yáalen káaldero'</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a10" TIME_SLOT_REF1="ts19" TIME_SLOT_REF2="ts20">
                <ANNOTATION_VALUE>chen ts'o'ojke' kin jo'oske paach apaarteilo' ku taja tun le tsaats ma'alobo' kin jo'osik</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a11" TIME_SLOT_REF1="ts21" TIME_SLOT_REF2="ts22">
                <ANNOTATION_VALUE>despwees tune' kin ka ts'ike máanteka te' káaldero, despweese' ki estenin in chokota ma'alobe máantekao' kin in ts'iku paacho' jats'uts u chen p'olk'aja ti'a chicharoon ku ya'alalo',</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a12" TIME_SLOT_REF1="ts23" TIME_SLOT_REF2="ts24">
                <ANNOTATION_VALUE>ti' esten tune je'elo' ken taake' e máako'obu mano'obo' jatsuts' u yiko'ob men nuuktak jats'uts ta jaantke' tsatsajki.</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a13" TIME_SLOT_REF1="ts25" TIME_SLOT_REF2="ts26">
                <ANNOTATION_VALUE>E k'e'no' ma' wa teen tseenki', manbilin ka' beetik ku ko'ono ten menin amigoso'obo' pus a... yaan oorae'  be... beintyseeis pesos u kiiloi u pyeji', leti' tune kin manik beyo' pus kin kiinsik, kin kaxkin kuxta yéete mejen paalalo'obo' ku manu k'a'ayto'ob tene bak'o' ku ma'ana ten way tin kaaja xk'a'atuno' way kin meyale'.</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a14" TIME_SLOT_REF1="ts27" TIME_SLOT_REF2="ts28">
                <ANNOTATION_VALUE>Pus tene' yaan oorae' kin bin ichkool wa yaan tu'ux kin  ta'ana beya' ki jan t'a'ana' te' kíinsajo' kin binin in kíinse pus aalgo ti' teen pus yo'osal in mejen paalalo'obo' yanin wik bix kenin in kuxta te'e... te'e estene aanyooso'oba' pus komo eske toojin wóole' yetin espoosae' pus kin wa'akti' ka taak xan u yáanten, yéete mejen paalalo'ob beyo' ku máanu t'oxo'ob le bak'o'.</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a15" TIME_SLOT_REF1="ts29" TIME_SLOT_REF2="ts30">
                <ANNOTATION_VALUE>Pus elo' te'e máako' ku bino'o meya te'e chi'ich'e'eno' wa ka'alak'o'opo' pus chen máanko'ob beyo' ku manko'o', tak máanteka yan oorae' ku k'áatko' tene' pus kin konik a kiiloi. doos kilosi, u p'ée boote bixu estenu mako' ten.</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a16" TIME_SLOT_REF1="ts31" TIME_SLOT_REF2="ts32">
                <ANNOTATION_VALUE>Pus elo' kin wike' ku manchaja pus a treeinta pesos u kiiloilu mako'ob, te'e tuno' te' kin kaxkin séentaboi', kin juntarku chan p'íit taak'ine' despweese' kin ka binin manu jeele k'éek'enin kiinso' </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a17" TIME_SLOT_REF1="ts33" TIME_SLOT_REF2="ts34">
                <ANNOTATION_VALUE>Te'e estene le puuro beyo' pus leu tojo ku jóok'o ten beyo' pus leeti' kin ka manik u jeele e k'e'no'ob in kiinso'.</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a18" TIME_SLOT_REF1="ts35" TIME_SLOT_REF2="ts36">
                <ANNOTATION_VALUE>pusee yaan oorae' ku taalin áamigoso' beyo' pus ts'u' yiko' bixin kiinske k'e'no'  ku t'ankeno', ko'ox kiinse k'e'no' ko'ox, pus tene' kin bin, yaan oorae' ken taake máako'ob ka'alak'o'opo'obo' e p'iste'obo' yaan oorae' kin bin, kin binin in kiinsti'ob pus ke in wooje beyo' ku t'ankeno', ts'u'u k'aóotkeno', tu'ux in taa, yaan oorae' ti' kamyon kin bin yaan oorae' ti' bíisikleta wa bixin estenin jan bin, yan oora pus yaan tenu káalderoile' pus faasil in jan binin kiins ti'ob.   </ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a19" TIME_SLOT_REF1="ts37" TIME_SLOT_REF2="ts38">
                <ANNOTATION_VALUE>Pus tene' kaada esten séemanal, sáabados, domiingos bix kin jan kiinsku tu k'éek'en,</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a20" TIME_SLOT_REF1="ts39" TIME_SLOT_REF2="ts40">
                <ANNOTATION_VALUE>pus esten kin wik ku ma'anaj, pus esten bix esten  je'elin beetke' bey kin kaxkin kuxtalo'ono' na'an u jeelu foormai.</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
        <ANNOTATION>
            <ALIGNABLE_ANNOTATION ANNOTATION_ID="a21" TIME_SLOT_REF1="ts41" TIME_SLOT_REF2="ts42">
                <ANNOTATION_VALUE>Pus te' diyaso'ob je'elo' pus... pus beyu u estenu  bin to'one diyaso'ob yéetelin estenin espoosao', desde tojak óolo'one' puru tu láaka beyo' yéete mejen paalalo'obo'.</ANNOTATION_VALUE>
            </ALIGNABLE_ANNOTATION>
        </ANNOTATION>
    </TIER>
